<% for (application of applications) { %>
    <% if (application.employee === username) { %>
        <% if (application.accepted) { %>
            <%- include("../application/_owned_application_accepted") %>
        <% } else { %>
            <%- include("../application/_owned_application") %>
        <% } %>
    <% } else { %>
        <% if (application.accepted) { %>
            <%- include("../application/_view_application_accepted") %>
        <% } else { %>
            <%- include("../application/_view_application") %>
        <% } %>
    <% } %>
<% } %>