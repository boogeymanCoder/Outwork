<% if (messages.error) { %>
    <div class="alert alert-danger alert-dismissible">
        <button type="button" class="close" data-dismiss="alert">&times;</button>
        <%= messages.error %>
  </div>
<% } %>

<% if (messages.info) { %>
    
  <div class="alert alert-success alert-dismissible">
    <button type="button" class="close" data-dismiss="alert">&times;</button>
    <%= messages.info %>
  </div>
<% } %>