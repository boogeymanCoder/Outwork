<div class="form-group">
    <div class="form-floating mb-3">
        <input  class="form-control mb-2" type="text" name="zipPostalCode" placeholder="Zip/Postal Code" value="<%= account.zipPostalCode %>" required>
        <div class="valid-feedback"></div>
        <div class="invalid-feedback">Please enter your zip or postal code.</div>
        <label class="form-label text-secondary" for="zipPostalCode"><small>Zip/Postal Code</small></label>
    </div>
</div>